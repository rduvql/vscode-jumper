{
	"name": "vsode-jumper",
	"displayName": "jumper",
	"description": "jumper extension for vscode",
    "version": "1.0.1",
    "license": "MIT",
    "icon": "images/icon.png",
    "publisher": "rduvql",
    "author": {
        "name": "rduvql"
    },
	"engines": {
		"vscode": "^1.40.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:ext.jumper.next",
		"onCommand:ext.jumper.previous"
    ],
    "keywords": [
        "jump",
        "jumper"
    ],
    "repository": {
        "type": "git",
        "url": "https://github.com/rduvql/vscode-jumper.git"
    },
    "bugs": {
        "url": "https://github.com/rduvql/vscode-jumper/issues"
    },
    "homepage": "https://github.com/rduvql/vscode-jumper/blob/master/README.md",
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "ext.jumper.next",
				"title": "next focus"
            },
            {
                "command": "ext.jumper.previous",
                "title": "previous focus"
            }
		]
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"postinstall": "node ./node_modules/vscode/bin/install",
		"test": "npm run compile && node ./node_modules/vscode/bin/test"
	},
	"devDependencies": {
		"typescript": "^3.1.4",
		"vscode": "^1.1.25",
		"tslint": "^5.8.0",
		"@types/node": "^8.10.25",
		"@types/mocha": "^2.2.42"
	}
}
